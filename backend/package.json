{
  "name": "todo-api",
  "version": "1.0.0",
  "description": "Real-time collaborative TODO list API",
  "main": "index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "type-check": "tsc --noEmit",
    "db:migrate": "node ./scripts/migrate.js",
    "db:seed": "node ./scripts/seed.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "hono": "^3.11.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231025.0",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2",
    "wrangler": "^4.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "todo",
    "api",
    "collaborative",
    "realtime",
