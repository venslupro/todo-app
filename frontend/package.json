    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.3",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.1",
    "jsdom": "^27.4.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^4.0.16"
  }
